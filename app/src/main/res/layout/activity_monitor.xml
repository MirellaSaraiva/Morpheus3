<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lelik_lima.morpheus.MonitorActivity">

    <TextView
        android:id="@+id/time_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="36dp"
        android:layout_marginStart="150dp"
        android:layout_marginTop="43dp"
        android:layout_below="@+id/ac_txt"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/actxt2"
        android:layout_below="@+id/actxt2"
        android:layout_marginTop="54dp"
        android:text="ON"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="104dp" />

    <TextView
        android:id="@+id/actxt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="188dp"
        android:layout_marginTop="51dp"
        android:layout_below="@+id/time_txt"
        android:layout_alignStart="@+id/time_txt" />

    <TextView
        android:id="@+id/ac_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/time_txt"
        android:layout_alignParentTop="true"
        android:layout_marginTop="52dp"
        android:text="TextView" />

    <Button
        android:id="@+id/wakebutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView2"
        android:layout_marginStart="132dp"
        android:layout_marginTop="46dp"
        android:text="Wake" />
</RelativeLayout>
